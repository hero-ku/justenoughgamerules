	"name": "JustEnoughGamerules",
	"description": "Adds more gamerules to configure your game.",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/justenoughgamerules/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "me.heroku.JustEnoughGameRules",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"justenoughgamerules.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.6",
		"minecraft": "~1.20",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.22"
